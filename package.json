{
  "name": "cozy-house",
  "version": "2.0.0",
  "description": "lending layout",
  "main": "webpack.config.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "test": "node test-server.ts",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "watch:p": "cross-env NODE_ENV=production webpack --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack",
    "dist": "gh-pages -d dist",
    "eslint": "npx eslint --fix src/.",
    "stats": "npx webpack --mode production --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslannn199/cozy-house.git"
  },
  "browserslist": [
    "last 2 version",
    "> 0.5%",
    "IE 10"
  ],
  "keywords": [
    "html",
    "css",
    "sass",
    "scss",
    "js",
    "javascript",
    "ts",
    "typescript"
  ],
  "author": "Ruslan Kamenskiy <ruslannn199@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ruslannn199/cozy-house/issues"
  },
  "homepage": "https://github.com/ruslannn199/cozy-house#readme",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "browserslist": "^4.21.7",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.2",
    "postcss-preset-env": "^8.4.2",
    "prettier": "2.2.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.4.3",
    "typescript": "5.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.15.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.js"
    }
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
